package com.litl.sdk.enum {
    /**
     * Constants used to denote the axis of interest of accelerometer values
     * for a KeypadButtonInterval.
     *
     * @see KeypadButtonInterval
     */
    public class AccelerometerAxis {
        /** the x-axis */
        public static const X:int = 0;

        /** the y-axis */
        public static const Y:int = 1;

        /** the z-axis */
        public static const Z:int = 2;
    }
}
